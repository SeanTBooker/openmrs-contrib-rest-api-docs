<span class="o">{</span>
  <span class="s2">"conceptReferenceTerm"</span>: <span class="s2">"21fb14d7-5cd9-3621-ac30-c9e57320e233"</span>,
  <span class="s2">"conceptMapType"</span>: <span class="s2">"35543629-7d8c-11e1-909d-c80aa9edcf4e"</span>
<span class="o">}</span>
